<h2>Login</h2>

<form class="form login-form">
  <div class="form-group">
    <label for="email-login">email:</label>
    <input class="form-input" type="text" id="email-login" />
  </div>
  <div class="form-group">
    <label for="password-login">password:</label>
    <input class="form-input" type="password" id="password-login" />
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="button" id="login-button">login</button>
    <button class="btn btn-primary" type="click" id="signup-button">Sign Up Instead</button>
  </div>
</form>

<script src="/js/login.js"></script>
<script src="/js/transferToSignup.js"></script>
